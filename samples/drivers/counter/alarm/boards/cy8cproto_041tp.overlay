&tcpwm0_0 {
	status = "okay";
	divider-type = <DIV_16_BIT>;
	divider-sel = <1>;
	divider-val = <11499>;
	interrupts = <16 0>;

	counter0_0 {
		status = "okay";
		compatible = "infineon,tcpwm-counter";
		clocks = <&peri_div_16bit_3>;
	};
};
